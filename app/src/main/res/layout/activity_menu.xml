<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name=".Historial Emocional"
        android:label="string/splash_activity_title"
        tools:context=".MenuActivity">

    <TextView
            android:text="@string/registro_emocional"
            android:layout_width="wrap_content"
            android:layout_height="29dp"
            android:id="@+id/txtHistorialLog" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="32dp" android:textSize="24sp"/>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="368dp"
            android:layout_height="434dp" android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@+id/txtHistorialLog" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp">
    </LinearLayout>
</android.support.constraint.ConstraintLayout>